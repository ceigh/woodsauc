{
  "sourceType": "module",
  "plugins": [
    "node_modules/jsdoc-babel"
  ],
  "source": {
    "include": [
      "src/js"
    ],
    "includePattern": ".+\\.js$"
  },
  "opts": {
    "encoding": "utf8",
    "recurse": true,
    "destination": "./docs",
    "readme": "JSDOC.md"
  },
  "templates": {
    "default": {
      "includeDate": false,
      "useLongnameInNav": true,
      "staticFiles": {
        "include": [
          "./demo.png"
        ]
      }
    }
  }
}
