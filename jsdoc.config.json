{
  "sourceType": "module",
  "plugins": [
    "node_modules/jsdoc-babel"
  ],
  "source": {
    "include": [
      "src/js"
    ],
    "includePattern": ".+\\.js$"
  },
  "opts": {
    "encoding": "utf8",
    "recurse": true,
    "destination": "./dev",
    "readme": "readmes/JSDOC.md"
  },
  "templates": {
    "default": {
      "includeDate": false,
      "useLongnameInNav": true
    }
  }
}
